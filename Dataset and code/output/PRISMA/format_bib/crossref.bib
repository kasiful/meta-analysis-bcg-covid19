@article{pop00314,
	author = {Aurélien Mazeraud and Bruno Gonçalves and Philippe Aegerter and Letizia Mancusi and Christine Rieu and Fernando Bozza and Khaoussou Sylla and Shidasp Siami and Tarek Sharshar},
	type = {journal-article},
	title = {Effect of early treatment with polyvalent immunoglobulin on acute respiratory distress syndrome associated with SARS-CoV-2 infections (ICAR trial): study protocol for a randomized controlled trial},
	journal = {Trials},
	publisher = {Springer Science and Business Media LLC},
	doi = {10.1186/s13063-021-05118-7},
	issn = {1745-6215},
	url = {http://dx.doi.org/10.1186/s13063-021-05118-7},
	fulltext = {http://link.springer.com/content/pdf/10.1186/s13063-021-05118-7.pdf},
	year = {2021},
	volume = {22},
	number = {1},
	abstract = {Background: As of mid-June 2020, 7,500,000 people were infected with SARS-CoV-2 worldwide and 420,000 people died, mainly from coronavirus disease 2019 (COVID-19)-related acute respiratory distress syndrome (ARDS). COVID-19-related ARDS is subject to a mortality rate of 50% and prolonged period of mechanical ventilation, with no specific pharmacological treatment currently available (Infection au nouveau Coronavirus (SARS-CoV-2), COVID-19, France et Monde. Methods: The trial is a phase III double-blind, randomized, multicenter, parallel group, concurrent, controlled study in hospitalized participants with COVID-19 requiring mechanical ventilation using a sequential design. Participants in the treatment group will receive infusions of polyvalent immunoglobulin for 4 consecutive days, and the placebo group will receive an equivalent volume of sodium chloride 0.9% for the same duration. The primary outcome is the number of ventilator-free days up to the 28th day. Secondary objectives are to evaluate the effect of IVIG on (1) organ failure according to the Sequential Organ Failure Assessment (SOFA) score at 14 and 28 days, (2) lung injury score at 14 and 28 days, (3) the occurrence of grade 3 or 4 adverse events of IVIG, (4) length of intensive care unit (ICU) stay, (5) length of hospital stay, (6) functional outcomes at day 90 defined by the activities of daily living and instrumental activities of the daily living scales, and (7) 90-day survival. One hundred thirty-eight subjects will be randomized in a 1:1 ratio to IVIG or placebo groups (69 in each group), considering 90% power, alpha level 0.05 (two sides), and 0.67 effect size level. Discussion: The ICAR trial investigates the effect of IVIG in COVID-19-related ARDS. We expect an increase in the survival rate and a reduction in the duration of mechanical ventilation, which is associated with significant morbidity. Trial registration: EudraCT 2020-001570-30. ClinicalTrials.gov},
	note = {4 cites: },
	note = {Query date: 2023-07-03 09:50:12},
}
